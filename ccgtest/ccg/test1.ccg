<ccg>
	<bsf file="../scripts/bean50main.rb" sourcefolder="/ccgtest/gen">
		<package name="de.tolina.uzb" clean="true">
			<bean name="Verbund">
				<property name="name" type="String" />
				<property name="nummer" type="int" />
			</bean>
			<bean name="Kunde">
				<property name="name" type="String" />
				<property name="nummer" type="int" />
	
				<property name="verbund" type="Verbund" />
				<property name="konto" type="Konto" cardinality="*" backref="kunde" />
			</bean>
			<bean name="Konto">
				<property name="name" type="String" />
				<property name="nummer" type="int" />
	
				<property name="kunde" type="Kunde" backref="konto" />
	
				<property name="zusatz" type="KontoZusatz" backref="konto" />
			</bean>
			<bean name="KontoZusatz">
				<property name="tgueberz" type="boolean" />
	
				<property name="konto" type="Konto" backref="zusatz" />
			</bean>
		</package>
	</bsf>
</ccg>
